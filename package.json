{
  "name": "@ruforms/primitives",
  "version": "0.0.0",
  "description": "primitives for RuForms",
  "keywords": [
    "ruforms",
    "react",
    "typescript",
    "forms"
  ],
  "author": "RuForms Team",
  "main": "./dist/index.js",
  "module": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.cjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "pre-push": "pnpm type-check && pnpm lint",
    "commitlint": "commitlint",
    "release": "semantic-release",
    "release:dry-run": "semantic-release --dry-run"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "tsup": "8.5.0",
    "typescript": "5.9.2",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint": "^9.33.0",
    "@eslint/js": "^9.33.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "globals": "^16.3.0",
    "@types/node": "^24.3.0",
    "semantic-release": "^24.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruforms/primitives.git"
  },
  "bugs": {
    "url": "https://github.com/ruforms/primitives/issues"
  },
  "homepage": "https://github.com/ruforms/primitives#readme",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "tsc --noEmit --pretty"
    ],
    "**/*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  }
}
